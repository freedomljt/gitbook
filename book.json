{
  "root": "./docs",
  "plugins": [
    "back-to-top-button",
    "collapsible-chapters",
    "isummary",
    "-lunr",
    "-search",
    "search-pro",
    "code",
    "splitter",
    "tbfed-pagefooter",
    "hide-element"
  ],
  "pluginsConfig": {
    "isummary":{
      "ignorePath": ["assets"]
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy Toobe 2020",
      "modify_label": "修改时间",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "toc2": {
      "addClass": true,
      "className": "toc"
    }
  }
}
