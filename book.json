{
  "title": "One book",
  "author": "Toobe Loong",
  "description": "A book",
  "root": "./docs",
  "plugins": [
    "back-to-top-button",
    "collapsible-chapters",
    "isummary",
    "-lunr",
    "-search",
    "search-pro",
    "code",
    "splitter",
    "tbfed-pagefooter",
    "hide-element",
    "insert-logo",
    "github",
    "custom-favicon",
    "theme-default"
  ],
  "pluginsConfig": {
    "favicon": "//freedomljt.github.io/gitbook/assets/favicon.ico",
    "github":{
      "url": "https://github.com/freedomljt"
    },
    "isummary":{
      "ignorePath": ["assets"]
    },
    "insert-logo":{
      "url": "//freedomljt.github.io/gitbook/assets/logo.jpg",
      "style": "background:none;width:80px;border-radius:50%;box-shadow:0 0 4px rgba(0,0,0,.4);"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy Toobe 2020",
      "modify_label": "修改时间",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "toc2": {
      "addClass": true,
      "className": "toc"
    }
  }
}
